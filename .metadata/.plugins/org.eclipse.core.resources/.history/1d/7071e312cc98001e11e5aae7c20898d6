package com.Banking;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;

 class Accounts
{
	String Accono;
	String AccoHolName;
	int accountBalance;
	ArrayList<Accounts> a1;
	
	
	Accounts()
	{
				
	}
	
	

	Accounts(String Accno, String AccoHolName, int AccountBanlance)
	{
		this.Accono=Accono;
		this.AccoHolName=AccoHolName;
		this.accountBalance=accountBalance;
		
	}
	
	
	
	
	public List<Accounts> createAccount(){
		
		
		a1.add(new Accounts("12345678","abc",123));
		a1.add(new Accounts("112223","xyz",102000));
		a1.add(new Accounts("6659885","sabc",12356));
		a1.add(new Accounts("55112223","dadf",102000));
		a1.add(new Accounts("6464","abc",12345666));
		a1.add(new Accounts("11256223","xydsz",102000));
		
		
		return a1;
		
		
		
	}
	public List<Accounts> createAccount(String Accno, String AccoHolName, int AccountBanlance){
		
		List<Accounts> user= new ArrayList<>();
		
		user.add(new Accounts(Accno, AccoHolName, AccountBanlance));
		
		return user;
		
		
	}

}


