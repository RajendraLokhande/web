package com.springcore.collections;

import org.springframework.context.ApplicationContext;
import org.springframework.contect.ClassPathXmlApplicationContect;

public class Test {

	public static <ApplicationConext> void main(String[] args) {

      ApplicationConext contet=new ClassPathXmlApplicationConext("com/springcore/collections/collectonconfig.xml");
	  Emp mp1 =(Emp)context.getBean("Emp1");
	  
      System.out.println(emp1,getName());
      System.out.println(emp1,getPhones());
      System.out.println(emp1,getAdresses());
      System.out.println(emp1,getCourses());
			
	}

}
